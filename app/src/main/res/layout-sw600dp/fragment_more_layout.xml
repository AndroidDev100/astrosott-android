                        android:textColor="@color/white"
                        android:textSize="@dimen/_16sp" />

                    <TextView
                        android:id="@+id/partnerBillingText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_VIP_user"
                        android:ellipsize="end"
                        android:fontFamily="@font/roboto_regular"
                        android:maxLines="1"
                        android:textAllCaps="false"
                        android:textColor="@color/yellow_orange"
                        android:textSize="12sp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/tv_subscribe_now"
                        android:layout_width="@dimen/_200sdp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/partnerBillingText"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/roboto_regular"
                        android:maxLines="2"
                        android:padding="@dimen/_1sdp"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="13sp" />
                </LinearLayout>


                <TextView
                    android:id="@+id/tv_renew_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_VIP_user"
